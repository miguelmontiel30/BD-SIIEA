                /*Cambio de contrase√±as*/

DELIMITER $$
CREATE PROCEDURE changed_pass(id_user VARCHAR(8), pass VARCHAR(15))
BEGIN
UPDATE usuarios SET contrasenia = pass WHERE usuarios.id_usuario = id_user;
END$$

CALL changed_pass("00000039","87654321")$$

                /*Cambio de correo y correo*/

DELIMITER $$
CREATE PROCEDURE user_update(ID VARCHAR(8), NAME VARCHAR(15), CORREO VARCHAR(25))
BEGIN
UPDATE usuarios SET email = CORREO, nombre = NAME WHERE usuarios.id_usuario = ID;
END$$

CALL changed_pass("00000039","Luis","fsdf@fdsf.com")$$
